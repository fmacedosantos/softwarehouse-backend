{
  "name": "softwarehouse",
  "version": "1.0.0",
  "description": "The system helps to pay the employees' salary.",
  "main": "src/server.js",
  "scripts": {
    "start": "nodemon src/server.js",
    "test": "jest --passWithNoTests",
    "config-sequilize": "npx sequelize-cli init",
    "create-migration": "npx sequelize-cli migration:create --name",
    "create-migrations": "npx sequelize-cli db:migrate",
    "delete-migrations": "npx sequelize-cli db:migrate:undo:all",
    "create-seed": "npx sequelize-cli seed:create --name",
    "create-seeds": "npx sequelize-cli db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "softwarehouse-sequelize-and-automatic-tests"
  },
  "keywords": [
    "['node'",
    "'express'",
    "'backend'",
    "'sequelize'",
    "'mysql'",
    "'jest']"
  ],
  "author": "fmacedosantos",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.12.0",
    "nodemon": "^3.1.9",
    "sequelize": "^6.37.5"
  }
}
